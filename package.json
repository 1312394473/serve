{
  "name": "micro-list",
  "version": "0.2.1",
  "description": "Render a list of files and directories",
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp build"
  },
  "files": [
    "dist",
    "views"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeit/micro-list.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator",
      "syntax-async-functions"
    ]
  },
  "bin": {
    "list": "dist/bin/list"
  },
  "keywords": [
    "now",
    "serve",
    "micro"
  ],
  "author": "leo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeit/micro-list/issues"
  },
  "homepage": "https://github.com/zeit/micro-list#readme",
  "devDependencies": {
    "babel-plugin-syntax-async-functions": "6.5.0",
    "babel-plugin-transform-async-to-generator": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-cached": "1.1.0",
    "gulp-chmod": "1.3.0",
    "gulp-sass": "2.3.1"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "filesize": "3.3.0",
    "handlebars": "4.0.5",
    "istextorbinary": "1.0.2",
    "micro-core": "0.1.0",
    "babel-regenerator-runtime": "6.5.0",
    "minimist": "1.2.0"
  }
}
