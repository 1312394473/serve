{
  "name": "micro-list",
  "version": "0.4.1",
  "description": "Single-command HTTP directory listing and file serving",
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp build",
    "test": "eslint bin/**"
  },
  "files": [
    "dist",
    "views"
  ],
  "repository": "zeit/micro-list",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator",
      "syntax-async-functions"
    ]
  },
  "bin": {
    "list": "dist/bin/list"
  },
  "eslintConfig": {
    "extends": "default",
    "parser": "babel-eslint"
  },
  "keywords": [
    "now",
    "serve",
    "micro",
    "http-server"
  ],
  "author": "leo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeit/micro-list/issues"
  },
  "homepage": "https://github.com/zeit/micro-list#readme",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-syntax-async-functions": "6.5.0",
    "babel-plugin-transform-async-to-generator": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "eslint": "^2.10.2",
    "eslint-config-default": "^0.1.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-cached": "1.1.0",
    "gulp-chmod": "1.3.0",
    "gulp-sass": "2.3.1"
  },
  "dependencies": {
    "args": "1.0.1",
    "babel-regenerator-runtime": "6.5.0",
    "chalk": "1.1.3",
    "filesize": "3.3.0",
    "handlebars": "4.0.5",
    "istextorbinary": "1.0.2",
    "micro-core": "0.1.0"
  }
}
